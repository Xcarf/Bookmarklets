javascript:alert("You activated edit page\nPress ctrl + q to turn it off"); document.body.contentEditable = "true"; const PressedKeys = {};document.addEventListener("keydown", KeyPressed);document.addEventListener("keyup", KeyReleased);function KeyPressed(ev) {    PressedKeys[ev.code] = true;    if (PressedKeys.KeyQ && PressedKeys.ControlLeft) {        alert("You turned off edit page mode\nRun the code again to activate it");        document.removeEventListener("keydown", KeyPressed);        document.removeEventListener("keyup", KeyReleased);        document.body.contentEditable = "false";     }}function KeyReleased(ev) {    delete PressedKeys[ev.code];}void 0;
